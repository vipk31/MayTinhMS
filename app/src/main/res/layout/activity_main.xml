        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_marginTop="64dp"
        app:layout_constraintTop_toBottomOf="@+id/txtKetQua" />

    <Button
        android:id="@+id/btn1"
        android:layout_width="61dp"
        android:layout_height="50dp"
